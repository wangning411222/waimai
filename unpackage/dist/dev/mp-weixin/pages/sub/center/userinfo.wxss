
.card {
  padding: 20rpx;
}
.right-img {
  /* height: 150rpx; */
  padding-left: 20rpx;
  /* overflow: hidden; */
}
.text-muted {
  font-size: 12px;
  color: #82848a;
}
.u-cell {
  position: relative;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  padding: 26rpx 32rpx;
  font-size: 28rpx;
  line-height: 54rpx;
  color: #606266;
  background-color: #fff;
  text-align: left;
}
.u-cell_title {
  font-size: 28rpx;
}
.u-cell__title,
.u-cell__value {
  flex: 1;
}
.u-cell__value {
  overflow: hidden;
  text-align: right;
  vertical-align: middle;
  color: #909399;
  font-size: 26rpx;
}

